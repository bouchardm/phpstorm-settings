<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="FONT_SCALE" value="1.0833334" />
    <option name="LINE_SPACING" value="1.6" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="IS_CAMEL_WORDS" value="true" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\breview\b.*">
      <option name="FOREGROUND" value="e326cd" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="e326cd" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?:@|#[\s]{0,}(@?)|\/\/(?:\s|@|\s@))revi(?:ew|sion).*">
      <option name="FOREGROUND" value="c000a8" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern pattern="(?:@|((\/\*)?\*(\s?)?)|#(\s?)|\/\/(\s?)(@?))todo.*" />
    <filter name="review">
      <pattern index="3" />
    </filter>
    <filter name="todo">
      <pattern index="4" />
    </filter>
  </component>
</application>